{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Information",
      "Microsoft.AspNetCore.Http": "Information",
      "Microsoft.AspNetCore.Routing": "Debug",
      "Voli.Api": "Debug",
      "Voli.Api.Controllers": "Debug",
      "Voli.Api.Services": "Debug",
      "Voli.Api.Repositories": "Debug"
    }
  },
  "AllowedHosts": "*",
  "CosmosDb": {
    "Endpoint": "https://your-cosmos-account.documents.azure.com:443/",
    "Key": "your-cosmos-primary-key",
    "DatabaseName": "voli"
  },
  "Auth": {
    "Authority": "https://login.microsoftonline.com/{tenant-id}",
    "Audience": "{client-id}"
  },
  "Cors": {
    "AllowedOrigins": [
      "http://localhost:3000",
      "https://localhost:3000"
    ]
  },
  "Seeding": {
    "AllowSeeding": true,
    "Secret": "your-seed-secret-here"
  }
}
